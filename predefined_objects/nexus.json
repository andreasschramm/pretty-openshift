{
	"kind": "List",
	"apiVersion": "v1",
	"metadata": {
		"name": "nexus",
		"annotations": {
			"description": "nexus definitions for axa infrastructure."
		}
	},

	"items":
	[
		{
			"kind": "BuildConfig",
			"apiVersion": "v1",
			"metadata": {
				"name": "nexus",
				"labels": {
					"application": "nexus"
				}
			},

			"spec": {
				"triggers":
				[
					{
						"type": "GitHub",
						"github": {
							"secret": "OEYVg1Y1Pwo11w7oeWdY"
						}
					},

					{
						"type": "Generic",
						"generic": {
							"secret": "GsoVtewlgIuatNaaFtrM"
						}
					},

					{
						"type": "ConfigChange"
					},

					{
						"type": "ImageChange",
						"imageChange": {

						}
					}
				],

				"source": {
					"type": "Git",
					"git": {
						"uri": "https://github.com/andreasschramm/pretty-openshift",
						"httpProxy": "http://default-http-proxy.pink.eu-central-1.aws.openpaas.axa-cloud.com:8888",
						"httpsProxy": "http://default-http-proxy.pink.eu-central-1.aws.openpaas.axa-cloud.com:8888"
					},

					"contextDir": "images/nexus-2"
				},

				"strategy": {
					"type": "Docker",
					"dockerStrategy": {
						"from": {
							"kind": "DockerImage",
							"name": "centos:7"
						},

						"env":
						[

							{
								"name": "http_proxy",
								"value": "http://default-http-proxy.pink.eu-central-1.aws.openpaas.axa-cloud.com:8888"
							},
							{
								"name": "HTTPS_PROXY",
								"value": "http://default-http-proxy.pink.eu-central-1.aws.openpaas.axa-cloud.com:8888"
							}
						]
					}
				},

				"output": {
					"to": {
						"kind": "ImageStreamTag",
						"name": "nexus:latest"
					}
				}
			}
		},

		{
			"kind": "ImageStream",
			"apiVersion": "v1",
			"metadata": {
				"name": "nexus",
				"labels": {
					"application": "nexus"
				}
			}
		},

		{
			"kind": "DeploymentConfig",
			"apiVersion": "v1",
			"metadata": {
				"name": "nexus",
				"labels": {
					"application": "nexus"
				}
			},

			"spec": {
				"strategy": {
					"type": "Rolling",
					"rollingParams": {
						"intervalSeconds": 1,
						"timeoutSeconds": 120
					}
				},

				"triggers":
				[
					{
						"type": "ConfigChange"
					},

					{
						"type": "ImageChange",
						"imageChangeParams": {
							"automatic": true,
							"containerNames":
							[
								"nexus"
							],

							"from": {
								"kind": "ImageStreamTag",
								"name": "nexus:latest"
							}
						}
					}
				],

				"replicas": 1,
				"selector": {
					"application": "nexus",
					"deploymentconfig": "nexus"
				},

				"template": {
					"metadata": {
						"creationTimestamp": null,
						"labels": {
							"application": "nexus",
							"deploymentconfig": "nexus"
						}
					},

					"spec": {
						"volumes":
						[
							{
								"name": "nexus-repositories-volume-1"
							}
						],

						"containers":
						[
							{
								"name": "nexus",
								"image": "nexus",
								"ports":
								[
									{
										"name": "http",
										"containerPort": 8081,
										"protocol": "TCP"
									}
								],

								"resources": {
									"limits": {
										"cpu": "500m",
										"memory": "1Gi"
									}
								},

								"volumeMounts":
								[
									{
										"name": "nexus-repositories-volume-1",
										"mountPath": "/repositories"
									}
								],

								"imagePullPolicy": "Always"
							}
						],

						"restartPolicy": "Always"
					}
				}
			}
		},

		{
			"kind": "Service",
			"apiVersion": "v1",
			"metadata": {
				"name": "nexus",
				"labels": {
					"application": "nexus"
				}
			},

			"spec": {
				"ports":
				[
					{
						"name": "8081-tcp",
						"protocol": "TCP",
						"port": 8081,
						"targetPort": 8081
					}
				],

				"selector": {
					"application": "nexus",
					"deploymentconfig": "nexus"
				}
			}
		},

